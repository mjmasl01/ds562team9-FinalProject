{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "ds562team9-datafactory"
		},
		"ABS_crypto_prices_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ABS_crypto_prices'"
		},
		"ABS_news_data_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ABS_news_data'"
		},
		"ABS_twitter_reddit_other_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ABS_twitter_reddit_other'"
		},
		"HttpServer_crypto_prices_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://pro-api.coinmarketcap.com/v1/"
		},
		"HttpServer_news_data_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://newsapi.org/v2/"
		},
		"HttpServer_twitter_reddit_other_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://api.tokenmetrics.com/v2/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ds562team9-datafactory_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ABS_crypto_prices_connectionString": {
						"value": "[parameters('ABS_crypto_prices_connectionString')]"
					},
					"ABS_news_data_connectionString": {
						"value": "[parameters('ABS_news_data_connectionString')]"
					},
					"ABS_twitter_reddit_other_connectionString": {
						"value": "[parameters('ABS_twitter_reddit_other_connectionString')]"
					},
					"HttpServer_crypto_prices_properties_typeProperties_url": {
						"value": "[parameters('HttpServer_crypto_prices_properties_typeProperties_url')]"
					},
					"HttpServer_news_data_properties_typeProperties_url": {
						"value": "[parameters('HttpServer_news_data_properties_typeProperties_url')]"
					},
					"HttpServer_twitter_reddit_other_properties_typeProperties_url": {
						"value": "[parameters('HttpServer_twitter_reddit_other_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}